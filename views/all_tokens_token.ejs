<tr>
  <td><a class="tokenid" href="/#token/<%= token.tokenDetails.tokenIdHex %>"><%= app.util.compress_tokenid(token.tokenDetails.tokenIdHex) %></a></td>
  <td class="nowrap"><%= token.tokenDetails.symbol %></td>
  <td class="nowrap"><%= app.util.compress_string(token.tokenDetails.name) %></td>
  <td><%= token.tokenStats.qty_valid_txns_since_genesis %></td>
  <td><%= token.tokenStats.qty_valid_token_utxos %></td>
  <td><%= token.tokenStats.qty_valid_token_addresses %></td>
  <td class="nowrap">
    <% if (token.tokenDetails.documentUri) { %>
      <% const url = app.util.document_link(token.tokenDetails.documentUri); %>
      <% if (url.length) { %>
        <a href="<%= url %>"><%= app.util.compress_string(token.tokenDetails.documentUri) %></a>
      <% } else { %>
        <%= app.util.compress_string(token.tokenDetails.documentUri) %>
      <% } %>
    <% } %>
  </td>
</tr>
